{
  "name": "@shabados/mobile",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=16",
    "npm": ">=7"
  },
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --color",
    "types": "tsc --pretty",
    "test": "cross-env ENVFILE=config/.env.local jest --color",
    "start:android": "cross-env ENVFILE=config/.env.local react-native run-android",
    "start:ios": "cross-env ENVFILE=config/.env.local react-native run-ios --simulator='iPhone 12'",
    "build:android": "bundle exec fastlane android build",
    "build:ios": "bundle exec fastlane ios build",
    "clean:android": "bundle exec fastlane android clean",
    "clean:ios": "bundle exec fastlane ios clean",
    "prepare-release:android": "bundle exec fastlane android prepare_release",
    "prepare-release:ios": "bundle exec fastlane ios prepare_release",
    "release:android": "bundle exec fastlane android release",
    "release:ios": "bundle exec fastlane ios release",
    "install-assets": "react-native link",
    "postinstall": "pod-install ios"
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/toolbar-android": "0.2.1",
    "@react-navigation/elements": "^1.3.1",
    "@react-navigation/material-bottom-tabs": "^6.0.9",
    "@react-navigation/material-top-tabs": "^6.1.1",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.5.0",
    "@splitsoftware/splitio-react-native": "^0.1.0",
    "env-var": "^7.1.1",
    "gurmukhi-utils": "^3.2.1",
    "i18next": "^21.6.11",
    "jotai": "^1.5.3",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-i18next": "^11.15.4",
    "react-native": "0.67.3",
    "react-native-animated-spinkit": "^1.5.2",
    "react-native-config": "^1.4.5",
    "react-native-device-info": "^8.4.8",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-localize": "^2.2.0",
    "react-native-pager-view": "^5.4.11",
    "react-native-reanimated": "^2.3.1",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.12.0",
    "react-native-tab-view": "^3.1.1",
    "react-native-vector-icons": "^9.0.0",
    "react-navigation-header-buttons": "^9.0.1",
    "react-query": "^3.33.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@faker-js/faker": "*",
    "@prettier/plugin-ruby": "^2.0.0",
    "@testing-library/react-native": "^8.0.0",
    "@types/jest": "^27.0.3",
    "@types/jest-when": "^2.7.3",
    "@types/lodash": "^4.14.177",
    "@types/react": "^17.0.37",
    "@types/react-native": "^0.66.6",
    "@types/react-native-vector-icons": "^6.4.10",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-jest": "^27.4.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "cross-env": "^7.0.3",
    "dotenv": "^12.0.3",
    "eslint": "^8.3.0",
    "eslint-config-airbnb": "^19.0.1",
    "eslint-config-airbnb-typescript": "^16.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "fishery": "^2.1.0",
    "husky": "^4.3.8",
    "jest": "^27.3.1",
    "jest-transform-stub": "^2.0.0",
    "jest-when": "^3.4.2",
    "jetifier": "^2.0.0",
    "metro-react-native-babel-preset": "^0.66.2",
    "pod-install": "^0.1.29",
    "prettier": "2.5.0",
    "react-test-renderer": "^17.0.2",
    "typescript": "^4.5.2"
  }
}
