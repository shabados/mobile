{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "~/test/*": ["./test/*"],
      "~/assets/*": ["./assets/*"],
      "~/*": ["./src/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app.config.js",
    "src-old/types/assets.d.ts.do"
  ]
}
